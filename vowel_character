#include<stdio.h>
int main()
{
	char ch;
	printf("enter a character");
	scanf("%c",&ch);
	int i;
	
	{
	if (ch == 'a'|| ch== 'b'||ch == 'c'||ch == 'd'||ch == 'e'||ch== 'A'|| ch == 'B'||ch == 'C'||ch == 'D'||ch == 'E')
	
    printf("the given character %c is vowel",ch);
    
    else
	
    printf("the given character %c is not vowel",ch);
    }

}
