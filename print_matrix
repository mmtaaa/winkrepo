#include<stdio.h>
int main()
{
    int m,n;
	int i,j, a[m][n];
	printf("enter the number of rows and columns of matrix\n");
	scanf("%d %d",&m,&n);
	
	
	printf("enter the element of matrix\n");
	for(i=0;i<m;i++)
	 {
	 	for(j=0;j<n;j++)
	 	{
	 	scanf("%d",&a[i][j]);
	    }
     }
	printf("matrix1:");
	for(i=0;i<m;i++)
	 	{
		 for(j=0;j<n;j++)
	 	  {
	 	   printf("%d \t",a[i][j]);
	      }
	      printf("\n");
	      
        }
        return 0;
}
