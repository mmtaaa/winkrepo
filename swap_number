#include<stdio.h>
int swap(int x, int y)
{
 int z;		

	z=x;
	x=y;
	y=z;
	printf("the value after swap is:%d %d",x,y);
return 0;
}
void main()
{
	int a,b,c;
	printf("enter the two numbers.\n");
	scanf(" %d %d",&a,&b);
	printf("the value before swap is: %d %d\n",a,b);
	
	swap(a,b); //calling function
}
